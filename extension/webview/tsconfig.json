{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": ["vite/client"]
  },
  "include": ["./**/*.ts", "./**/*.tsx", "../src/webview/**/*.ts", "../src/webview/**/*.tsx"],
  "exclude": ["../dist", "../node_modules"]
}

